<%= content_tag :h1, 'Welcome to Viewing Party!' %>
<%= content_tag :p, 'Description' %>
<%= link_to "New to Viewing Party? Register Here.", registration_path %>
<%= form_tag do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :password %>
  <%= text_field_tag :password %>
  <%= submit_tag 'Login' %>
<% end %>
