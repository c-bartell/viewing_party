<%= button_to 'Top Rated Movies', 'movies_path', method: :get, params: { some: 'params' } %>

<%= form_tag movies_path, method: :get do %>
  <%= text_field_tag :query, value: 'Movie Name' %>
  <%= submit_tag 'Search' %>
<% end %>

<% @movies.each do |movie| %>
  <section class='movie'>
    <p class='title'><%= link_to movie[:title], 'movies_show_path(movie.id)' %></p><br>
    <p class='rating'>Rating: <%= movie[:vote_average] %></p>
  </section>
<% end %>
